<!ELEMENT locales (locale+)>
<!ELEMENT locale EMPTY>
	<!ATTLIST locale
		key CDATA #REQUIRED
		name CDATA #REQUIRED
		iso639-2b CDATA #REQUIRED
		iso639-3 CDATA #REQUIRED
		complete (true | false) #IMPLIED
		stylesheet CDATA #IMPLIED>

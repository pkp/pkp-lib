msgid "admin.copyAccessLogFileTool.error.copyingFile"
msgstr "Napaka: kopiranje datoteke iz {$filePath} v {$tmpFilePath} ni bilo mogoče"

msgid "admin.copyAccessLogFileTool.error.deletingFile"
msgstr "Napaka: datoteke {$tmpFilePath} ni bilo mogoče odstraniti"

msgid "admin.copyAccessLogFileTool.warning.fileAlreadyExists"
msgstr "Opozorilo: datoteka {$filePath} že obstaja znotraj usageStats mape."

msgid "admin.copyAccessLogFileTool.success"
msgstr "Uspešno: {$filePath} v {$destinationPath}"

